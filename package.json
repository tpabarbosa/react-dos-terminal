{
    "name": "react-dos-terminal",
    "version": "0.1.4",
    "description": "A collection of React Components to simulate a DOS command line interface.",
    "repository": {
        "type": "git",
        "url": "https://github.com/tpabarbosa/react-dos-terminal"
    },
    "files": [
        "/dist"
    ],
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "rollup -c",
        "build-watch": "rollup -c -w",
        "start-dev-example": "cd dev-example && npm run start",
        "i-all": "npm i && cd dev-example && npm i",
        "dev": "cross-env NODE_ENV=dev npm-run-all --parallel build-watch start-dev-example "
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
